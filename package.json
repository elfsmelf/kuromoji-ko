{
  "name": "kuromoji-ko",
  "version": "1.0.0",
  "description": "Pure TypeScript Korean Morphological Analyzer - serverless compatible, based on kuromoji.js and mecab-ko-dic",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "dict"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:dict": "tsx scripts/build-dictionary.ts",
    "dev": "tsup src/index.ts --format esm --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "korean",
    "morphological-analysis",
    "tokenizer",
    "nlp",
    "mecab",
    "mecab-ko",
    "hangul",
    "kuromoji",
    "serverless",
    "nextjs",
    "vercel"
  ],
  "author": "Richard Paynter",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardpaynter/kuromoji-ko.git"
  },
  "bugs": {
    "url": "https://github.com/richardpaynter/kuromoji-ko/issues"
  },
  "homepage": "https://github.com/richardpaynter/kuromoji-ko#readme",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pako": "^2.0.4",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "doublearray": "^0.0.2",
    "pako": "^2.1.0"
  }
}
